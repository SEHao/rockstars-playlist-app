{
  "name": "music-playlist",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "ng serve",
    "start:backend": "json-server --watch ./example-data/db.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
